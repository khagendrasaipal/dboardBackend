<style type="text/css">  
.marquee {
    width: 100%;
  line-height: 50px;
  background-color: #2460b9;
  color: white;
    white-space: nowrap;
    overflow: hidden;
    box-sizing: border-box;
}
.marquee p {
    display: inline-block;
    padding-left: 100%;
    animation: marquee 60s linear infinite;
}
@keyframes marquee {
    0%   { transform: translate(0, 0); }
    100% { transform: translate(-100%, 0); }
}
</style>


<script type="text/javascript">

  function changeImageHere(){
    var element = document.getElementById('changeImage');

    if (element.src == "https://images.unsplash.com/photo-1646291417814-5241b938669a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80"){

        element.src = "https://images.unsplash.com/photo-1646282014691-620f9af448a8?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80";
    }
    else{
      element.src = "https://images.unsplash.com/photo-1646291417814-5241b938669a?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1170&q=80";
    }
    
  }

// Image Change in 15 seconds
  setInterval(changeImageHere, 5000);



</script>



